<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jsf="http://jsf.jd.com/schema/jsf" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
    http://jsf.jd.com/schema/jsf http://jsf.jd.com/schema/jsf/jsf.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

    <context:property-placeholder location="classpath:config/jsf.properties" ignore-unresolvable="true"/>

    <jsf:registry id="jsfRegistry" index="${jsf.registry.index}"/>

    <jsf:server id="jsf" protocol="jsf" threads="${jsf.server.threads}" threadpool="fixed"/>

    <jsf:consumer id="consumer12" interface="com.jd.test1.TestJsfApi" url="jsf://127.0.0.1:20220"
                  alias="test1" protocol="jsf" serialization="hessian"/>

</beans>